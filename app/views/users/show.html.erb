
    <div class="col-md-12">
      <div class="col-md-6">
        <span class="profile">
          <div class="profile_image">

          <h1 class = "test"><%= @user.username  %> </h1>
          <p> Member since: <%=@user.created_at.strftime("%F")%> </p>
          <img src= <%= @user.image%> >

        </div>
        </span>
      </div>
      <div class="col-md-6 ">
        <h1>Places I've climbed</h1>
        <div class="col-md-5 user_climb_list">
          <ul>
            <% @climbs.each do |f| %>
              <li><%= f.name %></li>

            <% end %>

          </ul>
          <%= link_to "Add new Location!", new_climb_path(@climb), :class => "btn btn-primary" %>

        </div>
      </div>
    </div>
